<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.liyakarimova.Controller">
   <children>
      <AnchorPane prefHeight="484.0" prefWidth="330.0">
         <HBox layoutX="14.0" layoutY="14.0" prefHeight="463.0" prefWidth="716.0">
            <children>
               <VBox spacing="10.0">
                  <children>
                     <HBox spacing="10.0">
                        <children>
                           <Button fx:id="clientUpButton" mnemonicParsing="false" onAction="#onClientUpButtonClicked" prefHeight="26.0" prefWidth="46.0">
                              <graphic>
                                 <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                       <Image url="@icons/298998111%C2%A0—%20копия.png" />
                                    </image>
                                 </ImageView>
                              </graphic></Button>
                           <TextField fx:id="clientPath" prefHeight="26.0" prefWidth="255.0" />
                        </children>
                     </HBox>
                     <TreeView fx:id="clientFileTree" onMouseClicked="#onTreeClicked" prefHeight="388.0" prefWidth="309.0" />
                  </children>
               </VBox>
               <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                  <children>
                     <Button fx:id="sendToCloudButton" mnemonicParsing="false" onAction="#onSendToCloudButtonClicked">
                        <graphic>
                           <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@icons/2989981.png" />
                              </image>
                           </ImageView>
                        </graphic></Button>
                     <Button mnemonicParsing="false" onAction="#onLoadFromCloudButtonClicked">
                        <graphic>
                           <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                              <image>
                                 <Image url="@icons/298998111.png" />
                              </image>
                           </ImageView>
                        </graphic></Button>
                  </children>
               </VBox>
               <VBox spacing="10.0">
                  <children>
                     <HBox spacing="10.0">
                        <children>
                           <Button fx:id="onUpCloudButtonClickedonUpCloudButtonClicked" mnemonicParsing="false" prefHeight="26.0" prefWidth="46.0">
                              <graphic>
                                 <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                       <Image url="@icons/298998111%C2%A0—%20копия.png" />
                                    </image>
                                 </ImageView>
                              </graphic></Button>
                           <TextField fx:id="cloudPath" prefHeight="26.0" prefWidth="269.0" />
                        </children>
                     </HBox>
                     <TreeView fx:id="cloudTreeView" prefHeight="392.0" prefWidth="298.0" />
                  </children>
               </VBox>
            </children>
         </HBox>
         <HBox layoutX="15.0" layoutY="482.0" prefHeight="44.0" prefWidth="301.0" spacing="50.0">
            <children>
               <Button fx:id="listRequestButton" mnemonicParsing="false" text="Список файлов " />
               <Button mnemonicParsing="false" onAction="#onLoginButtonClicked" text="Вход" />
            </children>
         </HBox>
         <padding>
            <Insets left="10.0" />
         </padding>
      </AnchorPane>
   </children>
</VBox>
